	"tokenColors": [
		{
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#7f7c8f"
			}
		},
		{
			"scope": [
				"string",
				"punctuation.definition.string",
			],
			"settings": {
				"foreground": "#d0402d"
			}
		},
		{
			"scope": [
				"constant.numeric"
